{
  "name": "myhealthapp-backend",
  "version": "0.0.1",
  "description": "Backend for MyHealthApp, with admin",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production && babel -d ./dist ./src/server",
    "start": "NODE_ENV=production nodemon --exec babel-node src/server/index.js",
    "start-deprecated": "npm run build && NODE_ENV=production node ./dist/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node src/server/index.js",
    "debug": "nodemon --inspect --exec babel-node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "coverage-upload": "codecov"
  },
  "author": "tdp2-2c-2019",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "nodemailer": "^6.3.0",
    "pg": "^7.12.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
